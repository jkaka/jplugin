<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <groupId>com.kaka.test</groupId>
    <artifactId>test</artifactId>
    <version>1.0-SNAPSHOT</version>
    <build>
        <plugins>
            <plugin>
                <groupId>com.kaka</groupId>
                <artifactId>autoCode-maven-plugin</artifactId>
                <version>1.0-SNAPSHOT</version>
                <configuration>
                    <codeVO>
                        <!--服务提供方-->
                        <sysCode>project-template</sysCode>
                        <rootPackage>com.kaka.template</rootPackage>
                        <!--服务调用方-->
                        <dubboConsumerSysCode>project-template-consumer</dubboConsumerSysCode>
                        <consumerRootPackage>com.kaka.template.consumer</consumerRootPackage>
                        <generatePath>E:\workspace\generateCode\project-template</generatePath>
                    </codeVO>
                    <dataSource>
                        <property>
                            <name>db_type</name>
                            <value>mysql</value>
                        </property>
                        <property>
                            <name>driver</name>
                            <value>com.mysql.jdbc.Driver</value>
                        </property>
                        <property>
                            <name>user</name>
                            <value>root</value>
                        </property>
                        <property>
                            <name>password</name>
                            <value>root</value>
                        </property>
                        <property>
                            <name>host</name>
                            <value>localhost:3306</value>
                        </property>
                        <property>
                            <name>dbname</name>
                            <value>jsk</value>
                        </property>
                    </dataSource>
                </configuration>
            </plugin>
        </plugins>
    </build>
</project>
